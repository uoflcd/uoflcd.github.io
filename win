[Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12
try { 
    $response = Invoke-WebRequest -Uri 'https://github.com/UofL-Cyber-Defense/CCDC-Public/raw/refs/heads/master/windows.zip' -UseBasicParsing
    break 
} catch {}

if (-not $response) {
    Write-Host "Failed to download files"
    return
}

Expand-Archive -Path windows.zip -DestinationPath "C:\Users\Administrator\Desktop"
rm windows.zip
